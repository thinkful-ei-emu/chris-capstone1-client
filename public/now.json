{
    "version": 2,
    "alias": "chris-mobile-bookshelf-app",
    "name": "mobile-bookshelf-app",
    "routes": [
        {
          "src": "^/static/(.*)",
          "dest": "/static/$1"
        },
        {
          "src": "^/favicon.ico$",
          "dest": "/favicon.ico"
        },
        {
          "src": ".*",
          "dest": "/index.html"
        }
    ]
  }